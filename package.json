{
  "name": "@mobily/wonka-extras",
  "version": "0.1.1",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "source": "./src/WonkaExtras.bs.js",
  "types": "./dist/types/src/WonkaExtras.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/types/src/WonkaExtras.d.ts",
      "source": "./src/WonkaExtras.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "*.md",
    "index.js.flow",
    "bsconfig.json",
    "package.json"
  ],
  "scripts": {
    "build": "bash ./scripts/build.sh",
    "esbuild": "node esbuild.config.js",
    "clean": "rm -rf ./dist",
    "test": "yarn install:test && yarn jest --coverage",
    "re:clean": "yarn bsb -clean-world",
    "re:build": "yarn bsb -make-world",
    "re:dev": "yarn bsb -make-world -w",
    "generate:flow": "node ./scripts/generate-flow-files.js",
    "install:test": "bash ./scripts/install-test.sh"
  },
  "keywords": [
    "wonka",
    "rescript",
    "typescript",
    "observable",
    "iterable",
    "stream"
  ],
  "author": "Marcin Dziewulski <hello@mobily.pl>",
  "homepage": "https://github.com/mobily/wonka-extras",
  "bugs": "https://github.com/mobily/wonka-extras/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobily/wonka-extras.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "wonka": "^4.0.15"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "babel-jest": "^26.6.3",
    "babel-plugin-closure-elimination": "^1.3.2",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "bs-platform": "^9.0.2",
    "esbuild": "^0.9.6",
    "flowgen": "^1.13.0",
    "gentype": "^3.44.0",
    "globby": "^11.0.2",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reason-test-framework": "^0.3.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
